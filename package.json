    "ember-source": "~3.24.0",
    "ember-styleguide": "^5.1.0",
    "ember-template-lint": "^2.18.0",
    "ember-tether": "^2.0.0",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
